  <script>
        // Global variables
        let currentLanguage = 'en';
        let currentRecordCount = 0;

        // Data SDK Handler
        const dataHandler = {
            onDataChanged(data) {
                currentRecordCount = data.length;
                console.log(`Data updated: ${data.length} records`);
            }
        };

        // Element SDK Configuration
        const defaultConfig = {
            page_title_en: "Premium Pig Feeds",
            page_title_sw: "Chakula cha Nguruwe cha Ubora",
            page_subtitle_en: "Accelerate growth and maximize feed efficiency with our scientifically formulated pig feeds",
            page_subtitle_sw: "Harakisha ukuaji na ongeza ufanisi wa chakula kwa chakula chetu cha nguruwe cha kisayansi",
            company_name: "Bounty Millers Limited",
            phone_number: "+254727455027",
            email_address: "info@bountymillers.com"
        };

        // Initialize SDKs
        async function initializeSDKs() {
            try {
                // Initialize Data SDK
                if (window.dataSdk) {
                    const initResult = await window.dataSdk.init(dataHandler);
                    if (!initResult.isOk) {
                        console.error("Failed to initialize Data SDK");
                    }
                }

                // Initialize Element SDK
                if (window.elementSdk) {
                    window.elementSdk.init({
                        defaultConfig,
                        onConfigChange: async (config) => {
                            // Update page titles
                            const pageTitleEn = document.getElementById('page-title-en');
                            if (pageTitleEn) {
                                pageTitleEn.textContent = config.page_title_en || defaultConfig.page_title_en;
                            }

                            const pageTitleSw = document.getElementById('page-title-sw');
                            if (pageTitleSw) {
                                pageTitleSw.textContent = config.page_title_sw || defaultConfig.page_title_sw;
                            }

                            const pageSubtitleEn = document.getElementById('page-subtitle-en');
                            if (pageSubtitleEn) {
                                pageSubtitleEn.textContent = config.page_subtitle_en || defaultConfig.page_subtitle_en;
                            }

                            const pageSubtitleSw = document.getElementById('page-subtitle-sw');
                            if (pageSubtitleSw) {
                                pageSubtitleSw.textContent = config.page_subtitle_sw || defaultConfig.page_subtitle_sw;
                            }

                            // Update company name
                            const companyElements = document.querySelectorAll('#company-name, #mobile-company-name, #footer-company-name');
                            companyElements.forEach(el => {
                                el.textContent = config.company_name || defaultConfig.company_name;
                            });

                            // Update contact information
                            const phoneElements = document.querySelectorAll('#phone-display, #mobile-phone-display, #footer-phone');
                            phoneElements.forEach(el => {
                                el.textContent = config.phone_number || defaultConfig.phone_number;
                                if (el.tagName === 'A') {
                                    el.href = `tel:${config.phone_number || defaultConfig.phone_number}`;
                                }
                            });

                            const emailElements = document.querySelectorAll('#footer-email');
                            emailElements.forEach(el => {
                                el.textContent = config.email_address || defaultConfig.email_address;
                                if (el.tagName === 'A') {
                                    el.href = `mailto:${config.email_address || defaultConfig.email_address}`;
                                }
                            });
                        },
                        mapToCapabilities: (config) => ({
                            recolorables: [],
                            borderables: [],
                            fontEditable: undefined,
                            fontSizeable: undefined
                        }),
                        mapToEditPanelValues: (config) => new Map([
                            ["page_title_en", config.page_title_en || defaultConfig.page_title_en],
                            ["page_title_sw", config.page_title_sw || defaultConfig.page_title_sw],
                            ["page_subtitle_en", config.page_subtitle_en || defaultConfig.page_subtitle_en],
                            ["page_subtitle_sw", config.page_subtitle_sw || defaultConfig.page_subtitle_sw],
                            ["company_name", config.company_name || defaultConfig.company_name],
                            ["phone_number", config.phone_number || defaultConfig.phone_number],
                            ["email_address", config.email_address || defaultConfig.email_address]
                        ])
                    });
                }
            } catch (error) {
                console.error("SDK initialization error:", error);
            }
        }

        // Initialize everything when page loads
        document.addEventListener('DOMContentLoaded', function() {
            initializeSDKs();
            initializeAOS();
            initializeLanguageToggle();
            initializeMobileMenu();
            initializeTabs();
            initializeCharts();
        });

        // AOS Animation initialization
        function initializeAOS() {
            AOS.init({
                duration: 800,
                easing: 'ease-in-out',
                once: true,
                offset: 100
            });
        }

        // Language toggle functionality
        function initializeLanguageToggle() {
            const toggles = document.querySelectorAll('#language-toggle, #mobile-language-toggle');
            toggles.forEach(toggle => {
                toggle.addEventListener('change', function() {
                    currentLanguage = this.checked ? 'sw' : 'en';
                    updateLanguage();
                    // Sync both toggles
                    toggles.forEach(t => t.checked = this.checked);
                });
            });
        }

        function updateLanguage() {
            // Hide all language elements first
            document.querySelectorAll('.lang-en, .lang-sw').forEach(el => {
                el.classList.add('hidden-lang');
            });

            // Show current language elements
            document.querySelectorAll(`.lang-${currentLanguage}`).forEach(el => {
                el.classList.remove('hidden-lang');
            });

            // Update elements with data attributes
            document.querySelectorAll('[data-lang-en][data-lang-sw]').forEach(el => {
                const text = currentLanguage === 'en' ? el.getAttribute('data-lang-en') : el.getAttribute('data-lang-sw');
                el.textContent = text;
            });
        }

        // Mobile menu functionality
        function initializeMobileMenu() {
            const mobileMenuBtn = document.getElementById('mobile-menu-btn');
            const closeMobileMenuBtn = document.getElementById('close-mobile-menu');
            const mobileMenu = document.getElementById('mobile-menu');
            
            mobileMenuBtn.addEventListener('click', function() {
                mobileMenu.classList.add('active');
            });

            closeMobileMenuBtn.addEventListener('click', function() {
                mobileMenu.classList.remove('active');
            });

            // Close menu when clicking outside
            mobileMenu.addEventListener('click', function(e) {
                if (e.target === mobileMenu) {
                    mobileMenu.classList.remove('active');
                }
            });
        }

        // Tab functionality
        function initializeTabs() {
            const tabButtons = document.querySelectorAll('.tab-button');
            const tabContents = document.querySelectorAll('.tab-content');

            tabButtons.forEach(button => {
                button.addEventListener('click', function() {
                    const targetTab = this.getAttribute('data-tab');
                    
                    // Remove active class from all buttons and contents
                    tabButtons.forEach(btn => btn.classList.remove('active'));
                    tabContents.forEach(content => content.classList.remove('active'));
                    
                    // Add active class to clicked button and corresponding content
                    this.classList.add('active');
                    document.getElementById(targetTab).classList.add('active');
                });
            });
        }

        // Initialize charts
        function initializeCharts() {
            // Growth Chart
            const growthCtx = document.getElementById('growthChart');
            if (growthCtx) {
                new Chart(growthCtx, {
                    type: 'line',
                    data: {
                        labels: ['Week 1', 'Week 4', 'Week 8', 'Week 12', 'Week 16', 'Week 20', 'Week 24'],
                        datasets: [{
                            label: 'Weight (kg)',
                            data: [5, 15, 25, 45, 65, 85, 120],
                            borderColor: 'rgba(251, 146, 60, 1)',
                            backgroundColor: 'rgba(251, 146, 60, 0.1)',
                            borderWidth: 3,
                            fill: true,
                            tension: 0.4
                        }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        scales: {
                            y: {
                                beginAtZero: true,
                                title: {
                                    display: true,
                                    text: 'Weight (kg)'
                                }
                            },
                            x: {
                                title: {
                                    display: true,
                                    text: 'Age (Weeks)'
                                }
                            }
                        },
                        plugins: {
                            legend: {
                                display: false
                            }
                        }
                    }
                });
            }

            // Feed Efficiency Chart
            const feedEfficiencyCtx = document.getElementById('feedEfficiencyChart');
            if (feedEfficiencyCtx) {
                new Chart(feedEfficiencyCtx, {
                    type: 'bar',
                    data: {
                        labels: ['Starter (5-25kg)', 'Grower (25-60kg)', 'Finisher (60-120kg)'],
                        datasets: [{
                            label: 'Daily Feed (kg)',
                            data: [1.0, 2.5, 4.5],
                            backgroundColor: 'rgba(146, 64, 14, 0.8)',
                            borderColor: 'rgba(146, 64, 14, 1)',
                            borderWidth: 1,
                            yAxisID: 'y'
                        }, {
                            label: 'Daily Gain (g)',
                            data: [550, 850, 750],
                            backgroundColor: 'rgba(251, 146, 60, 0.8)',
                            borderColor: 'rgba(251, 146, 60, 1)',
                            borderWidth: 1,
                            yAxisID: 'y1'
                        }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        scales: {
                            y: {
                                type: 'linear',
                                display: true,
                                position: 'left',
                                title: {
                                    display: true,
                                    text: 'Daily Feed (kg)'
                                }
                            },
                            y1: {
                                type: 'linear',
                                display: true,
                                position: 'right',
                                title: {
                                    display: true,
                                    text: 'Daily Gain (g)'
                                },
                                grid: {
                                    drawOnChartArea: false,
                                }
                            }
                        },
                        plugins: {
                            legend: {
                                display: true
                            }
                        }
                    }
                });
            }
        }

        // Quote request function
        async function requestQuote(productName) {
            if (currentRecordCount >= 999) {
                showToast(currentLanguage === 'en' ? 'Maximum limit reached. Please contact us directly.' : 'Kikomo cha juu kimefikiwa. Tafadhali wasiliana nasi moja kwa moja.', 'error');
                return;
            }

            // Show loading state
            const button = event.target;
            const originalText = button.innerHTML;
            button.innerHTML = '<span class="spinner"></span> ' + (currentLanguage === 'en' ? 'Submitting...' : 'Inatuma...');
            button.disabled = true;

            const data = {
                type: 'quote_request',
                product: productName,
                animal_type: 'pigs',
                created_at: new Date().toISOString()
            };

            if (window.dataSdk) {
                const result = await window.dataSdk.create(data);
                if (result.isOk) {
                    showToast(currentLanguage === 'en' ? 'Quote request submitted! We\'ll contact you soon.' : 'Ombi la bei limewasilishwa! Tutawasiliana nawe hivi karibuni.', 'success');
                } else {
                    showToast(currentLanguage === 'en' ? 'Failed to submit request. Please try again.' : 'Imeshindwa kutuma ombi. Tafadhali jaribu tena.', 'error');
                }
            } else {
                showToast(currentLanguage === 'en' ? 'Quote request noted! Please contact us for pricing.' : 'Ombi la bei limerekodiwa! Tafadhali wasiliana nasi kwa bei.', 'success');
            }

            // Reset button state
            button.innerHTML = originalText;
            button.disabled = false;
        }

        // WhatsApp function
        function openWhatsApp(product) {
            const phone = window.elementSdk?.config?.phone_number || defaultConfig.phone_number;
            const message = currentLanguage === 'en' ? 
                `Hello Bounty Millers! I'm interested in ${product} pig feed. Can you provide more information about nutritional content, pricing, and availability for swine production?` :
                `Hujambo Bounty Millers! Ninapenda ${product} chakula cha nguruwe. Je, mnaweza kutoa maelezo zaidi kuhusu maudhui ya lishe, bei, na upatikanaji kwa uzalishaji wa nguruwe?`;
            const url = `https://wa.me/${phone.replace(/[^0-9]/g, '')}?text=${encodeURIComponent(message)}`;
            window.open(url, '_blank', 'noopener,noreferrer');
        }

        // Toast notification function
        function showToast(message, type = 'info') {
            const toast = document.createElement('div');
            toast.className = `fixed top-20 right-4 z-50 p-4 rounded-lg shadow-lg max-w-sm transform translate-x-full transition-transform duration-300`;
            
            const colors = {
                success: 'bg-green-600 text-white',
                error: 'bg-red-600 text-white',
                warning: 'bg-yellow-600 text-white',
                info: 'bg-blue-600 text-white'
            };
            
            toast.className += ` ${colors[type] || colors.info}`;
            toast.innerHTML = `
                <div class="flex items-center">
                    <i class="fas fa-${type === 'success' ? 'check-circle' : type === 'error' ? 'exclamation-circle' : type === 'warning' ? 'exclamation-triangle' : 'info-circle'} mr-3"></i>
                    <span>${message}</span>
                </div>
            `;
            
            document.body.appendChild(toast);
            
            // Animate in
            setTimeout(() => {
                toast.style.transform = 'translateX(0)';
            }, 100);
            
            // Animate out and remove
            setTimeout(() => {
                toast.style.transform = 'translateX(100%)';
                setTimeout(() => {
                    if (document.body.contains(toast)) {
                        document.body.removeChild(toast);
                    }
                }, 300);
            }, 4000);
        }
